{
    "taskGroups": [
        {
            "taskSpec": {
                "runnables": [
                    {
                        "container": {
                            "imageUri": "europe-west1-docker.pkg.dev/protocol-labs-data/pl-data/filet:v0.4.0",
                            "volumes": [
                                "/mnt/share:/gcs"
                            ],
                            "options": "--privileged -e LILY_BLOCKSTORE_CACHE_SIZE=2000000 -e LILY_STATESTORE_CACHE_SIZE=800000"
                        }
                    },
                    {
                        "script": {
                            "text": "ls /mnt/share"
                        }
                    }
                ],
                "computeResource": {
                    "cpuMilli": 48000,
                    "memoryMib": 190000,
                    "boot_disk_mib": 300000
                },
                "volumes": [
                    {
                        "gcs": {
                            "remotePath": "pl-data-temp"
                        },
                        "mountPath": "/mnt/share"
                    }
                ],
                "maxRetryCount": 1,
                "maxRunDuration": "86400s"
            },
            "taskCount": 1,
            "parallelism": 1
        }
    ],
    "allocationPolicy": {
        "instances": [
            {
                "policy": {
                    "machineType": "n2-standard-48",
                    "provisioningModel": "SPOT"
                }
            }
        ]
    },
    "logsPolicy": {
        "destination": "CLOUD_LOGGING"
    }
}
